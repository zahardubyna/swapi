{
  "name": "swapi",
  "version": "0.0.1",
  "description": "",
  "author": "Fish",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && typeorm -d dist/database/datasource.config.js",
    "migration:generate": "npm run typeorm -- migration:generate database/migrations/migration",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "re:migrate": "npm run migration:revert && rm database/migrations/* && npm run migration:generate && npm run migration:run",
    "seed:config": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js config -d database/seed.config.ts database/seeds/*.ts",
    "seed:pack": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d database/seed.config.ts database/seeds/*.ts",
    "seed:build": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register database/seeds/relations/relations.builder.seed.ts"
  },
  "dependencies": {
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@aws-sdk/client-s3": "^3.417.0",
    "@aws-sdk/s3-request-presigner": "^3.418.0",
    "@casl/ability": "^6.7.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "@types/ms": "^0.7.34",
    "@types/passport-jwt": "^3.0.10",
    "@types/uuid": "^9.0.7",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "ioredis": "^5.4.1",
    "js-sha256": "^0.10.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "ms": "^2.1.3",
    "mysql2": "^3.6.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "secrets": "^1.1.5",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@users(.*)": "<rootDir>/src/users/$1",
      "@casl(.*)": "<rootDir>/src/casl/$1",
      "@auth(.*)": "<rootDir>/src/auth/$1",
      "@entities(.*)": "<rootDir>/src/entities/$1",
      "@file.services(.*)": "<rootDir>/src/file.sevices/$1",
      "@decorators(.*)": "<rootDir>/src/decorators/$1",
      "@database(.*)": "<rootDir>/database/$1"
    }
  },
  "_moduleAliases": {
    "@users": ".src/users",
    "@casl": ".src/casl",
    "@auth": ".src/auth",
    "@entities": ".src/entities",
    "@file.services": ".src/file.services",
    "@decorators": ".src/decorators",
    "@database": "./database"
  }
}